pushd .. > /dev/null 2>&1
. ./all_env.sh > /dev/null 2>&1
. ./setenv.sh > /dev/null 2>&1
popd > /dev/null 2>&1

PADO_PROPERTIES="-Dpado.home.dir=$PADO_HOME"
$GF_JAVA -Xmx256m -cp $CLASSPATH $PADO_PROPERTIES com.netcrest.pado.biz.file.FileSplitterUnix $*
